<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/padding1"
    android:paddingLeft="@dimen/padding3"
    android:paddingRight="@dimen/padding3"
    android:paddingStart="@dimen/padding3"
    android:paddingEnd="@dimen/padding3"
    android:gravity="center_vertical"
    android:paddingBottom="@dimen/padding1"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:orientation="vertical">

            <hawaiiappbuilders.c.view.OpenSansBoldTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_7"
                android:textColor="@color/app_grey_dark"
                android:text="Time"/>

            <hawaiiappbuilders.c.view.OpenSansTextView
                android:id="@+id/tvDelTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_7"
                android:textColor="@color/app_grey_dark"
                android:text="Name"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="4"
            android:orientation="vertical">

            <hawaiiappbuilders.c.view.OpenSansBoldTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_7"
                android:textColor="@color/app_grey_dark"
                android:text="Customer"/>

            <hawaiiappbuilders.c.view.OpenSansTextView
                android:id="@+id/tvDelCustomer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_7"
                android:textColor="@color/app_grey_dark"
                android:text="Name"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="4"
            android:orientation="vertical">

            <hawaiiappbuilders.c.view.OpenSansBoldTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_7"
                android:textColor="@color/app_grey_dark"
                android:text="Status"/>

            <hawaiiappbuilders.c.view.OpenSansTextView
                android:id="@+id/tvDelStatus"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_7"
                android:textColor="@color/app_grey_dark"
                android:text="Name"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:orientation="vertical">

            <hawaiiappbuilders.c.view.OpenSansBoldTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_7"
                android:textColor="@color/app_grey_dark"
                android:text="Bid"/>

            <hawaiiappbuilders.c.view.OpenSansTextView
                android:id="@+id/tvDelBid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_7"
                android:textColor="@color/app_grey_dark"
                android:text="Name"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:orientation="vertical">

            <hawaiiappbuilders.c.view.OpenSansBoldTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_7"
                android:textColor="@color/app_grey_dark"
                android:text="Rating"/>

            <hawaiiappbuilders.c.view.OpenSansTextView
                android:id="@+id/tvDelRating"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_7"
                android:textColor="@color/app_grey_dark"
                android:text="Rating"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:orientation="vertical">

            <hawaiiappbuilders.c.view.OpenSansBoldTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_7"
                android:textColor="@color/app_grey_dark"
                android:text="Tip"/>

            <hawaiiappbuilders.c.view.OpenSansTextView
                android:id="@+id/tvDelTip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_7"
                android:textColor="@color/app_grey_dark"
                android:text="Tip"/>
        </LinearLayout>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="@dimen/padding1"
        android:background="@color/app_grey_dark"/>
</LinearLayout>